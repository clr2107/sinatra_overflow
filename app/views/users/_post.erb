<article id="<%= question.id %>">
  <form method="post" action='/questions/<%= question.id %>/vote' class="inline">
    <button type="submit" name="submit_param" value="submit_value" class="fa fa-sort-desc vote-button upvote-button"></button>
  </form>
  <h2><a href='/questions/<%= question.id %>'><%= question.question_text %></a></h2>
  <p>
    <span class='author'>By: <%= question.user.first_name %> <%= question.user.last_name %></span>
    <br>
    <span class='votes'>Votes: <%= question.votes.count %></span>
    <a class="delete" href='/questions/<%= question.id %>'>delete</a>
  </p>
</article>
